

<div class="row-fluid">
	<div class="span12">

			<table id ="viewme" class="table table-striped">
			<thead>
				   
			</thead>
			<tbody>	
			
				
			<%File.open("userspace/" + @username + '/' + @file, "r") do |infile| -%>
				<% limit = 0 -%>
				<% while (line = infile.gets) -%>
        			<% if (line =~ /##/) -%>	
	        		<% else -%>
        			<tr>
        			<td>
	        			<%= line %><br/>	        	
    				    <% limit = limit + 1 -%>
    				<% end -%>
        			</td>
        			</tr>
        			<% if (limit == 50) -%>
        				<% break -%>
        			<% end -%>	
	    		<% end -%>
			<% end -%>
			</tbody>
			</table>
		<script>  var tf = new TableFilter('demo',  {case_sensitive: true 	
				 	base_path: '../../vendor/assets/});	
				 	tf.init();	
				</script>";
		
		
		
	</div>
</div>

